{
    "name": "demo/auction",
    "description": "Demo project",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "php-di/php-di": "6.4",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.14",
        "symfony/console": "^5.4",
        "symfony/var-dumper": "^5.4"
    },
    "require-dev": {
        "overtrue/phplint": "^3.4",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "app": "php bin/app.php --ansi",
        "lint": "phplint",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }
}
